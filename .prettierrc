{
	"printWidth": 100,
	"semi": true,
	"useTabs": true,
	"tabWidth": 4,
	"singleAttributePerLine": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"importOrder": [
		"^react",
		"<THIRD_PARTY_MODULES>",
		"(^@/(models|types)(/.*)?$)|(.*types.*)",
		"(^@/(schemas)(/.*)?$)|(.*schema.*)",
		"^@/constants/(.*)$",
		"^@/core/(.*)$",
		"^@/hooks/(.*)$",
		"^@/utils/(.*)$",
		"^@/services/(.*)$",
		"^@/store(/.*)?$",
		"^@/pages/(.*)$",
		"^@/components(/.*)?(?<!\\.(css|scss|sass|less))$",
		"^\\./.*$",
		"^@/assets/(.*)$",
		"\\.(css|scss|sass|less)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
